@model FirstASPNET5AppWithVSCode.Models.MenuItem[]
@{ ViewData["Title"] = "Home Page"; }
<div class="row">
    <div class="col-md-12 text-center">
        <h1>ASP NET 5</h1>
        <hr>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <p>My first ASP NET 5 App with Visual Studio Code</p>
        <p>MVC 6</p>
    </div>
    <div class="col-md-6">
        <p>Visual Studio Code</p>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <hr>
        <ul>
            @foreach(var item in Model)
            {
                <li><a href="@item.Url">@item.Label</a></li>
            }
        </ul>
    </div>
</div>